		"isbot": "^4.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^7.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@hono/vite-dev-server": "^0.18.1",
		"@playwright/test": "^1.48.2",
		"@react-router/dev": "^7.0.1",
